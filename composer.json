{
	"name": "fostercommerce/global-content-menu-item",
	"description": "Relocates the 'Global Content' single above 'Entries'.",
	"type": "craft-plugin",
	"license": "mit",
	"version": "1.0.0",
	"support": {
		"email": "support@fostercommerce.com"
	},
	"require": {
		"craftcms/cms": "^5.0"
	},
	"autoload": {
		"psr-4": {
			"fostercommerce\\globalcontentmenuitem\\": "src/"
		}
	},
	"extra": {
		"name": "Global Content Menu Item",
		"handle": "global-content-menu-item",
		"description": "Relocates a 'Global Content' single above 'Entries'.",
		"version": "1.0.0",
		"class": "fostercommerce\\globalcontentmenuitem\\GlobalContentMenuItem"
	},
	"scripts": {
		"phpstan": "phpstan --memory-limit=1G",
		"ecs-check": "ecs check --ansi --memory-limit=1G",
		"ecs-fix": "ecs check --ansi --fix --memory-limit=1G",
		"rector": "rector process --config rector.php",
		"rector-dry-run": "rector process --dry-run --config rector.php"
	}
}

